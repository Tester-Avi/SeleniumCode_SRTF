public class ConfigDataProviders {

	FileInputStream fis;
	
	public ConfigDataProviders () {
		
		File src = new File(".\\Configuration\\config.properties");
		
		try {
			fis = new FileInputStream(src);
		}
		
		
		
	}
	
	
}
